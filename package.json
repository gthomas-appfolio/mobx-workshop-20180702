{
  "name": "appfolio-react-template",
  "private": true,
  "version": "0.4.0",
  "description": "Appfolio template for ES2015, React, Webpack, and Babel.",
  "author": "Appfolio, Inc.",
  "license": "UNLICENSED",
  "main": "dev_server.js",
  "scripts": {
    "dist:dev": "webpack --progress",
    "dist": "webpack -p",
    "install_modules": "npm install",
    "install_offline": "npm install --prefer-offline",
    "lint": "eslint --format './node_modules/eslint-teamcity/index.js' src test",
    "lint:dev": "eslint src test",
    "start": "node dev_server.js",
    "test": "mocha --recursive --timeout 10000 --compilers js:babel-register --require ignore-styles --require ./test/helpers.js",
    "test:watch": "mocha --watch --recursive --timeout 10000 --compilers js:babel-register --require ignore-styles --require ./test/helpers.js",
    "watch": "webpack --config webpack.dev.config --watch"
  },
  "dependencies": {
    "babel-polyfill": "~6.23.0",
    "es6-promise": "~4.1.1",
    "eslint-import-resolver-webpack": "^0.8.4",
    "mobx": "^3.2.1",
    "mobx-react": "^4.2.2",
    "prop-types": "^15.6.1",
    "react": "^15.6.2",
    "react-apm": "^1.4.1",
    "react-dom": "^15.6.2",
    "react-gears": "^2.21.0",
    "whatwg-fetch": "~2.0.2"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "autoprefixer": "~7.1.1",
    "babel-core": "~6.25.0",
    "babel-eslint": "~8.2.3",
    "babel-loader": "~7.1.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "~0.28.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15.4": "^1.0.5",
    "eslint": "^4.9.0",
    "eslint-config-appfolio-react": "^1.0.3",
    "eslint-teamcity": "^1.3.1",
    "express": "~4.15.3",
    "fecha": "^2.3.0",
    "ignore-styles": "~5.0.1",
    "jsdom": "~11.1.0",
    "jsdom-global": "~3.0.2",
    "lodash": "~4.17.4",
    "mocha": "^5.0.4",
    "node-sass": "^4.7.2",
    "nyc": "~11.0.3",
    "postcss": "~6.0.8",
    "postcss-loader": "^2.1.3",
    "react-addons-test-utils": "^15.4.0",
    "sass-loader": "~6.0.5",
    "style-loader": "~0.18.1",
    "webpack": "~2.7.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "~2.18.2",
    "webpack-visualizer-plugin": "~0.1.10"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "dev_server.js",
      "webpack.*config.js"
    ]
  }
}
